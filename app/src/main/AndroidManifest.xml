<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.concretecrackrepairguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ConcreteCrackRepairGuide">
        <activity android:name=".Reptech.Overlays"></activity>
        <activity android:name=".Reptech.FlexibleSealing" />
        <activity android:name=".Reptech.Stitching" />
        <activity android:name=".Reptech.Grouting" />
        <activity android:name=".Reptech.RoutingAndSealing" />
        <activity android:name=".Reptech.EpoxyInjection" />
        <activity android:name=".Activities.CrackDetails" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>